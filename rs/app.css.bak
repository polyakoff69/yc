.menu-bar {
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
    -fx-background-color: #2A579A;
    -fx-color: white;
}

/* .menu {
    -fx-background-color: #123456;
} */

.menu:showing {
    -fx-background-color: #3E6DB6;
}

.menu:hover {
    -fx-background-color: #3E6DB6;
}

.menu .label {
    -fx-text-fill: white;
}

/* .menu:showing .label {
    -fx-background-color: #123456
} */

.menu-item:focused {
    -fx-background-color: #3E6DB6;
}

.menu-item:focused .label {
    -fx-background-color: #3E6DB6;
    -fx-text-fill: white;
}

.menu-item .label {
    -fx-text-fill: black;
}


.tool-bar {
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
    -fx-background-color: #2A579A;
}

.tool-bar .button {
    -fx-background-color: transparent;
}

.tool-bar .button:hover{
    -fx-background-color: -fx-shadow-highlight-color, -fx-body-color;
    -fx-background-insets: 0 0 -1 0, 0, 1, 2;
    -fx-background-radius: 20px;
    -fx-padding: 0.333333em 0.666667em 0.333333em 0.666667em; /* 4 8 4 8 */
    -fx-text-fill: -fx-text-base-color;
    -fx-alignment: CENTER;
    -fx-content-display: LEFT;
}


.fbar {
    -fx-background-color: -fx-control;
}

.fbar .button:hover{
    -fx-background-color: -fx-shadow-highlight-color, -fx-body-color;
    -fx-background-insets: 0 0 -1 0, 0, 1, 2;
    -fx-background-radius: 6px;
    -fx-padding: 0.333333em 0.666667em 0.333333em 0.666667em; /* 4 8 4 8 */
    -fx-text-fill: -fx-text-base-color;
    -fx-alignment: CENTER;
    -fx-content-display: LEFT;
}


.dialog-pane {
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
}

.dialog-pane > .button-bar > .container {
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
}

.dialog-pane > .content.label {
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
}

.dialog-pane:header .header-panel {
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
}

.dialog-pane:header .header-panel .label{
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
}

.dialog-pane .button{
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
}

.dialog-pane .button:hover{

}


.main-split-pane {
    -fx-box-border: linear-gradient(#2A579A, #ccc);
}

.main-split-pane .split-pane-divider {
    -fx-border: none;
    -fx-padding: 0 2px 0 2px;
    // -fx-background-color: transparent, -fx-control;
    -fx-background-color: linear-gradient(#2A579A, #ccc);
    -fx-background-insets: 0, 1 0 1 0;
}


.tab-pane {
    -fx-border-insets: 0 0 0 0;
    -fx-border-width: 0;
    -fx-border-color: transparent;
    -fx-background-color: linear-gradient(#2A579A, #ccc);
    -fx-box-border: transparent;
}

*.tab-header-area  {
    -fx-padding: 0;
    -fx-background-color: transparent;
}

*.tab-header-background {
    -fx-padding: 0;
    -fx-background-color: transparent;
}

.tab {
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
    -fx-background-color: #3E6DB6;
    -fx-border-color: #3E6DB6;
    -fx-border-width: 0 0 0 0;
    -fx-border-insets: 0 0 0 0;
}

.tab:selected, .tab:focused {
    -fx-background-color: #ccc;
    -fx-focus-color: transparent;
    -fx-border-color: mediumblue;
    -fx-border-width: 4 0 0 0;
    -fx-border-insets: 0 0 0 0;
}

.tab-label {
    -fx-text-fill: white ;
}

.tab:selected .tab-label {
    -fx-text-fill: -fx-text-base-color;
}


.table-view{
    -fx-font-size: 11;
    -fx-font-family: "Segoe UI Semilight", "Ubuntu", Tahoma;
}


.file-panel-status {
  -fx-background-color: #ccc;
}


.file-panel-toolbar {
  -fx-background-color: #ccc;
}

.file-panel-toolbar-left {
    -fx-border-width: 0 1 4 0;
    -fx-border-color: #2A579A;
}

.file-panel-toolbar-right {
    -fx-border-width: 0 4 1 0;
    -fx-border-color: #2A579A;
}


.tooltip{
    -fx-background-radius: 0 0 0 0;
    -fx-background-color: yellow;
    -fx-border-color: black;
    -fx-border-width: 1 1 1 1;
    -fx-text-fill: black;
}